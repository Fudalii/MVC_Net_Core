@model ServiceType

@{
    ViewData["Title"] = "Create";
}

<br />
<h2>Add Servie Type</h2>
<hr />

<form asp-action="Create">
    <div class="form-border">
       
        <div class="form-group">
            <div class="row">
                <div class="col-sm-1">
                    <label asp-for="Name" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="Name" disabled="disabled" class="form-control" />
                </div>
            </div>

        </div>
        <br />
        <div class="form-group">
            <a asp-action="Edit" class="btn btn-sm btn-primary">Edit</a>
            <a asp-action="Index" class="btn btn-sm btn-success">Back to List</a>
        </div>
    </div>
</form>